{
  "manifest_version": 2,

  "name": "QuickLink",
  "description": "This extension allows you to quickly shorten and/or customize your url",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "QuickLink"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "tabs",
    "notifications"
  ]
}